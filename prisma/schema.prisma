generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DB_DATABASE_URL")
  directUrl = env("DB_DIRECT_URL")
}

model profile {
  id        String   @id @default(uuid())
  name      String
  email     String   @unique
  userId    String   @unique
  birthDate DateTime
  active    Boolean
}
